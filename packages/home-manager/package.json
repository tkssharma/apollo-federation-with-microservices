{
  "name": "@mobileleaplabs/home-manager",
  "version": "1.1.0",
  "description": "home manager app to manage homes",
  "license": "UNLICENSED",
  "engines": {
    "node": "12.x || 14.x",
    "npm": ">=6.9.x"
  },
  "devEngines": {
    "node": "12.x || 14.x",
    "npm": ">=6.9.x"
  },
  "scripts": {
    "start": "nest start",
    "prestart": "npm run migration:run",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "tsc -p tsconfig.build.json",
    "check-engines": "node ../../node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prebuild": "npm run check-engines && npm run clean",
    "build:prod": "npm run build",
    "start:prod": "node dist/src/main.js",
    "clean": "rimraf dist",
    "test": "npm run test:migration && npm run test:ci && npm run test:e2e",
    "test:unit": "jest",
    "test:migration": "dotenv -e env.test --  typeorm migration:run -t each",
    "test:ci": "NODE_ENV=CI jest --ci none --testPathPattern='src' --forceExit true --collectCoverage --maxWorkers 2 --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config-e2e.js --runInBand --silent --detectOpenHandles",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --quiet",
    "prettier": "./node_modules/.bin/prettier --check \"**/*.{js,json,ts,yml,yaml}\"",
    "prettier:write": "./node_modules/.bin/prettier --write \"**/*.{js,json,ts,yml,yaml}\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:generate": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run -t each",
    "migration:revert": "typeorm migration:revert",
    "db:sync": "npm run typeorm:cli schema:sync"
  },
  "keywords": [
    "typescript",
    "document-repository"
  ],
  "dependencies": {
    "@apollo/federation": "^0.36.1",
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.2.2",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.2",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.2.2",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.0.2",
    "@profusion/apollo-federation-upload": "^4.0.0",
    "apollo-server-core": "^3.7.0",
    "apollo-server-express": "^2.25.2",
    "aws-sdk": "^2.1140.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "express": "4.17.3",
    "graphql": "^15.8.0",
    "graphql-tool": "^1.0.0",
    "graphql-tools": "^8.2.8",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "ts-morph": "^14.0.0",
    "typeorm": "^0.2.41",
    "url-join-ts": "^1.0.5",
    "uuidv4": "6.2.12",
    "winston": "^3.7.2",
    "yamljs": "^0.3.0"
  },
  "resolutions": {
    "**/**/fs-capacitor": "^6.2.0",
    "**/graphql-upload": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^27.0.2",
    "@types/joi": "^17.2.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.11.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fbjs-scripts": "^3.0.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "mock-jwks": "^1.0.1",
    "nock": "^13.2.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "_moduleAliases": {
    "@app": "dist/src/app",
    "@auth": "dist/src/app/auth",
    "@blob": "dist/src/app/blob",
    "@core": "dist/src/app/core",
    "@domain": "dist/src/app/domain",
    "@logger": "dist/src/logger",
    "@config": "dist/src/config",
    "@db": "dist/src/db"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
