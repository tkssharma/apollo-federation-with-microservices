{
  "name": "notifications-api",
  "version": "1.0.0",
  "description": "notifications apis",
  "engines": {
    "node": "12.x || 14.x",
    "npm": ">=6.9.x"
  },
  "devEngines": {
    "node": "12.x || 14.x",
    "npm": ">=6.9.x"
  },
  "scripts": {
    "start": "nest start",
    "copy:templates": "sh ./scripts/copy-templates.sh",
    "postbuild": "npm run copy:templates",
    "prestart:dev": "npm run copy:templates",
    "prestart": "npm run db:migrate:run",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "tsc -p tsconfig.build.json",
    "check-engines": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prebuild": "npm run check-engines && npm run clean",
    "build:prod": "npm run build",
    "start:prod": "node dist/src/main.js",
    "clean": "rimraf dist/src/",
    "pretest": "npm run test:migration",
    "test": "npm run test:migration && npm run test:ci",
    "test:unit": "jest",
    "test:migration": "dotenv -e env.test --  node_modules/.bin/knex migrate:latest",
    "test:ci": "NODE_ENV=CI jest --ci none --testPathPattern='src' --forceExit true --collectCoverage --maxWorkers 2 --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config-e2e.js --runInBand  --detectOpenHandles",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --quiet",
    "prettier": "./node_modules/.bin/prettier --check \"**/*.{js,json,ts,yml,yaml}\"",
    "prettier:write": "./node_modules/.bin/prettier --write \"**/*.{js,json,ts,yml,yaml}\"",
    "prepare": "husky install",
    "copy-files": "copyfiles -e test/fixtures/*.ts -u 1 test/fixtures/**  dist/src/test",
    "db:rollback": "dotenv -- node_modules/.bin/knex migrate:rollback",
    "db:migrate:make": "dotenv -- node_modules/.bin/knex migrate:make",
    "db:migrate:run": "dotenv -- node_modules/.bin/knex migrate:latest",
    "db:seed:make": "dotenv -- node_modules/.bin/knex seed:make",
    "db:seed:run": "dotenv -- node_modules/.bin/knex seed:run"
  },
  "keywords": [
    "typescript",
    "notification-repository"
  ],
  "dependencies": {
    "@nanogiants/nestjs-swagger-api-exception-decorator": "1.5.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "1.0.2",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/terminus": "^7.1.0",
    "@ntegral/nestjs-sendgrid": "^1.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "fbjs-scripts": "3.0.1",
    "handlebars": "4.7.7",
    "helmet": "^4.4.1",
    "joi": "17.4.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.9",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "moment": "^2.29.1",
    "nest-knexjs": "0.0.5",
    "nestjs-rollbar": "1.4.1",
    "nodemailer": "6.7.5",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7",
    "superagent-defaults": "^0.1.14",
    "swagger-ui-express": "^4.1.3",
    "ts-jest": "27.0.4",
    "ts-node": "10.8.1",
    "url-join-ts": "^1.0.5",
    "uuid": "8.3.2",
    "uuidv4": "^6.2.6",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "_moduleAliases": {
    "@logging": "dist/src/logging",
    "@app": "dist/src/app",
    "@auth": "dist/src/auth",
    "@config": "dist/src/config",
    "@domain": "dist/src/app/domain",
    "@core": "dist/src/app/core",
    "@shared": "dist/src/app/shared",
    "@storage": "dist/src/storage"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/auth0": "2.34.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.2",
    "@types/jest": "27.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "4.14.172",
    "@types/node": "^12.12.29",
    "@types/nodemailer": "6.4.4",
    "@types/supertest": "^2.0.8",
    "@types/triple-beam": "^1.3.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "^7.0.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "typescript": "4.2.4"
  }
}
