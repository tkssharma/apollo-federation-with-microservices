{
  "name": "@mobileleaplabs/drivers",
  "version": "0.1.0",
  "description": "Drivers used to power up services",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "check-engines": "node ../../node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "clean": "rimraf build && rimraf coverage",
    "prebuild": "npm run clean && npm run check-engines",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "npm run test:ci",
    "test:ci": "jest --ci --collectCoverage --maxWorkers 2 --passWithNoTests"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "dependencies": {
    "@azure/storage-blob": "12.8.0",
    "@ntegral/nestjs-sendgrid": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "rollbar": "^2.24.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/common": "^8.4.2",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.4.2",
    "@nestjs/testing": "^8.2.5",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0",
    "@nestjs/config": "^1.0.0",
    "@nestjs/testing": "^7.0.0 || ^8.0.0",
    "reflect-metadata": "~0.1",
    "rxjs": "^6.0.0 || ^7.0.0"
  }
}
